{
  "name": "Majesti Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Majesti extension",
  "homepage_url": "http://github.com/thomashare/majesti-ext",
  "icons": {
    "16": "static/png/icons/icon16.png",
    "48": "static/png/icons/icon48.png",
    "128": "static/png/icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "script": "static/js/background.js"
  },
  "options_page": "pages/options.html",
  "browser_action": {
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "http://google.com/#q=*"
  ]
}