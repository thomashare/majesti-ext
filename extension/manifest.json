{
  "name": "Googlr!",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Googlr! Extensions",
  "homepage_url": "http://google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "script": "src/background.js"
  },
  "options_page": "pages/options.html",
  "permissions": [
    "http://google.com/#q=*"
  ]
}